{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "lib": [
            "ESNext",
            "DOM"
        ],
        "allowJs": true,
        "checkJs": false,
        "noEmit": true,
        "skipLibCheck": true,
    },
    "include": [
        "src/**/*.js"
    ],
    "exclude": [
        "node_modules",
        "src/gen/jsrouter.gen.js"
    ]
}